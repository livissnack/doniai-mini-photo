/* pages/redpack/index.wxss */
.redpack-pic {
  background-color: #e0554b;
  border-radius: 8px;
  width: 100%;
  height: 160px;
  background-size: 100% auto;
  overflow: hidden;
}

.redpack-pic:hover {
  transform: rotate(2deg);
}

.hb {
  position: relative;
}

.hb::after {
  content: "";
  display: inline-block;
  vertical-align: middle;
  width: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
  height: 100%;
  background-repeat: no-repeat;
  background-position: bottom;
  background-size: 110% auto;
  border-radius: 8px;
  background-image: url(data:image/png;base64,);
}


.redpack-name {
  width: 100%;
  text-align: center;
  opacity: .9;
  font-size: 14px;
  line-height: 24px;
  color: #000;
  margin-top: 24px;
}